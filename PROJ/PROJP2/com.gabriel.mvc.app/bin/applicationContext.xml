<?xml version="1.0" encoding="UTF-8"?>  
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
  
<bean id="carloc" class="com.gabriel.mvc.shapesfx.Location">  
<property name="x" value="0"></property>  
<property name="y" value="0"></property>  
</bean>  
<bean id="bodyloc" class="com.gabriel.mvc.shapesfx.Location">  
<property name="x" value="100"></property>  
<property name="y" value="100"></property>  
</bean>  
<bean id="lamploc" class="com.gabriel.mvc.shapesfx.Location">  
<property name="x" value="250"></property>  
<property name="y" value="110"></property>  
</bean>  
<bean id="frontwheelloc" class="com.gabriel.mvc.shapesfx.Location">  
<property name="x" value="190"></property>  
<property name="y" value="125"></property>  
</bean>  
<bean id="backwheelloc" class="com.gabriel.mvc.shapesfx.Location">  
<property name="x" value="110"></property>  
<property name="y" value="125"></property>  
</bean>  

<bean id="polytoploc" class="com.gabriel.mvc.shapesfx.Location">  
<property name="x" value="50"></property>  
<property name="y" value="0"></property>  
</bean>  



<bean id="carbody" class="com.gabriel.mvc.shapeimpl.CarBody">  
<property name="location" ref="bodyloc"></property>  
<property name="length" value="50"></property>  
<property name="width" value="150"></property>  
<property name="border" value="10"></property>
</bean>  


<bean id="lamp" class="com.gabriel.mvc.shapeimpl.Wheel">  
<property name="location" ref="lamploc"></property>  
<property name="radius" value="20"></property>    
</bean>  


<bean id="frontwheel" class="com.gabriel.mvc.shapeimpl.Wheel">  
<property name="location" ref="frontwheelloc"></property>  
<property name="radius" value="30"></property>    
<property name="outRadius" value="50"></property>    
</bean>  

<bean id="backwheel" class="com.gabriel.mvc.shapeimpl.Wheel">  
<property name="location" ref="backwheelloc"></property>  
<property name="radius" value="30"></property>    
<property name="outRadius" value="50"></property>    
</bean>  

<bean id="polyTop" class="com.gabriel.mvc.shapeimpl.Polytop">  
<property name="location" ref="polytoploc"></property> 
<property name="x1" value="50,50,150,200"></property>  
<property name="y1" value="100,45,45,100"></property>  
<property name="points" value="4"></property>
</bean>  

<bean id="car" class="com.gabriel.mvc.shapeimpl.Car">  
<property name="location" ref="carloc"></property>
<property name="frontWheel" ref="frontwheel"></property>
<property name="backWheel" ref="backwheel"></property>  
<property name="body" ref="carbody"></property>  
<property name="Lamp" ref="lamp"></property>
<property name="polytop" ref="polyTop"></property>

</bean>  
  
</beans>  